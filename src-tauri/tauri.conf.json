{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "auto-sleeper",
  "version": "0.1.0",
  "identifier": "cn.begit.sleeper",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Auto Sleeper",
        "width": 400,
        "height": 300,
        "visible": false,
        "fullscreen": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
    "plugins": {
    }
}
